function G = ggPenalty(Point,gamma)
%求Penalty检验函数的二阶导G
%Point为n*1的x矩阵
[n1,n2] = size(Point);
if n1==0 && n2==0
    error('error input Point');
end
G=zeros(n1,n2);
Gfunc1 = @(x,i) 8*x(i,1)*x;
Gfunc2 = @(x,i,gamma) 2*gamma+4*sum(x.*x)+2*x(i,1)*x(i,1)-1
i=1;
while i<n
    G(i,:) = Gfunc1(Point,i);
    G(i,i) = Gfunc2(Point,i,gamma);
end

